@page "/routingexample"
@page "/newroutingexample"
@page "/routingexample/{parameter1}"
@page "/routingexample/{parameter1}/{parameter2}"

    
<h3>RoutingExample</h3>
<h4>Parameter 1 - @Parameter1</h4>
<h4>Parameter 2 - @Parameter2</h4>
